local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

local UIS = game:GetService("UserInputService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local folder = workspace:FindFirstChild("InvisibleWalls")
local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")


local ENABLED = false
local HITBOX_SIZE = 10
local TRANSPARENCY = 0.35
local DEFAULT_SIZE = Vector3.new(2, 1, 1)
local HITBOX_COLOR = Color3.fromRGB(255, 60, 60)


local function getRoot(character)
	return character and character:FindFirstChild("HumanoidRootPart")
end

local function applyToCharacter(character)
	local root = getRoot(character)
	if not root or not root:IsA("BasePart") then return end

	root.CanCollide = false

	if ENABLED then
		root.Size = Vector3.new(HITBOX_SIZE, HITBOX_SIZE, HITBOX_SIZE)
		root.Transparency = TRANSPARENCY
		root.Material = Enum.Material.Neon
		root.Color = HITBOX_COLOR
	else
		root.Size = DEFAULT_SIZE
		root.Transparency = 0
		root.Material = Enum.Material.Plastic
	end
end

local function applyToAll()
	for _, player in pairs(Players:GetPlayers()) do
		if player ~= LocalPlayer and player.Character then
			applyToCharacter(player.Character)
		end
	end
end

local function removewalls()
    if folder then
        folder:Destroy()
    end
end


local Window = WindUI:CreateWindow({
	Title = "TeeHub - Slap Duels",
	Icon = "activity",
	Folder = "teehub",
	Size = UDim2.fromOffset(580, 460),
	MinSize = Vector2.new(560, 350),
	MaxSize = Vector2.new(850, 560),
	Transparent = true,
	Theme = "Dark",
	Resizable = true,
	SideBarWidth = 200,
	BackgroundImageTransparency = 0.42,
	HideSearchBar = true,
	ScrollBarEnabled = false,
    User = {
        Enabled = true,
         Callback = function()
            WindUI:Notify({
                Title = "Your Profile",
                Content = "literally you lol",
                Duration = 3,
            })
        end
     }
})

local Tab4 = Window:Tab({ Title = "Main", Icon = "box", Locked = false })
local Tab = Window:Tab({ Title = "Player", Icon = "user", Locked = false })
local Tab2 = Window:Tab({ Title = "Teleports", Icon = "cloud", Locked = false })
local Tab3 = Window:Tab({ Title = "Misc", Icon = "cpu", Locked = false })


Tab:Toggle({
	Title = "Hitbox",
	Desc = "Enable custom hitboxes",
	Icon = "x",
	Type = "Checkbox",
	Value = false,
	Callback = function(state)
		ENABLED = state
		applyToAll()
	end
})


Tab:Slider({
	Title = "Hitbox Size",
	Desc = "Changes the size of the hitboxes",
	Step = 1,
	Value = {
		Min = 5,
		Max = 15,
		Default = HITBOX_SIZE,
	},
	Callback = function(value)
		HITBOX_SIZE = value
		applyToAll() 
	end
})

Tab:Slider({
	Title = "Walk Speed",
	Step = 1,
	Value = {
		Min = 16,
		Max = 150,
		Default = humanoid.WalkSpeed,
	},
	Callback = function(value)
		if humanoid then
			humanoid.WalkSpeed = value
		end
	end
})

Tab:Slider({
	Title = "Jump Height",
	Step = 1,
	Value = {
		Min = 7.2,
		Max = 200,
		Default = humanoid.JumpHeight,
	},
	Callback = function(value)
		if humanoid then
			humanoid.UseJumpPower = false
			humanoid.JumpHeight = value
		end
	end
})

local Button = Tab3:Button({
    Title = "Destroy invisible walls",
    Desc = "Removes invisible walls in the lobby",
    Locked = false,
    Callback = removewalls
})

local Keybind = Tab3:Keybind({
    Title = "Close Window",
    Desc = "Keybind to close & open menu",
    Value = "RightAlt",
    Callback = function(v)
        Window:SetToggleKey(Enum.KeyCode[v])
    end
})

local Paragraph = Tab2:Paragraph({
    Title = "Work In Progress",
    Image = "",
    ImageSize = 30,
    Thumbnail = "",
    ThumbnailSize = 80,
    Locked = true,
})

Tab:Toggle({
	Title = "Auto Win",
	Icon = "x",
	Type = "Checkbox",
	Value = false,
	Locked = true,
})

loadstring(game:HttpGet("https://raw.githubusercontent.com/g70xl0l/teehub/refs/heads/main/version"))()
